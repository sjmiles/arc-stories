import 'https://$shell/artifacts/Common/List.manifest'

import './data/PlaidTransaction.schema'

particle PlaidTransactionItem in './source/PlaidTransactionItem.js'
  in PlaidTransaction transaction
  consume item

// if we supply an item *and* a multiplexed-item, then generic containers
// can consume either format

recipe MuxedPlaidTransactionItem
  copy #transactions as items
  ItemMultiplexer
    list = items
    hostedParticle = PlaidTransactionItem

