import 'https://$cdn/artifacts/Common/List.manifest'

import './data/PlaidAccount.schema'

particle PlaidAccountItem in './source/PlaidAccountItem.js'
  in PlaidAccount account
  consume item

// if we supply an item *and* a multiplexed-item, then generic containers
// can consume either format
recipe MuxedPlaidAccountItem
  map #accounts as items
  ItemMultiplexer
    list = items
    hostedParticle = PlaidAccountItem
    consume item
  description `show list items`

